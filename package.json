{
  "name": "indoor-positioning-cordova-plugin",
  "private": true,
  "version": "1.0.0",
  "description": "Cordova plugin for indoor positioning",
  "keywords": [
    "cordova",
    "indoor",
    "positioning"
  ],
  "license": "MIT",
  "scripts": {
    "build": "NODE_ENV=${NODE_ENV:-production} rollup --config config/rollup.js --watch",
    "test": "NODE_ENV=${NODE_ENV:-test} bin/ci",
    "test:unit": "NODE_ENV=${NODE_ENV:-test} karma start config/karma.js --single-run",
    "test:watch": "NODE_ENV=${NODE_ENV:-test} karma start config/karma.js --watch",
    "lint": "eslint src/ test/"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "cordova": "^9.0.0",
    "eslint": "^6.0.1",
    "eslint-plugin-compat": "^3.3.0",
    "eslint-plugin-prettier": "^3.0.0",
    "karma": "^6.3.16",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^3.0.0",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.5",
    "karma-rollup-preprocessor": "^7.0.0",
    "karma-sinon": "^1.0.5",
    "mocha": "^6.1.4",
    "prettier": "^1.10.2",
    "rollup": "^1.17.0",
    "rollup-plugin-buble": "^0.19.4",
    "rollup-plugin-node-resolve": "^5.2.0",
    "sinon": "^7.1.1",
    "sinon-chai": "^3.2.0"
  },
  "dependencies": {}
}
