{
  "name": "indoor-positioning-cordova-plugin",
  "private": true,
  "repository": "https://gitlab.com/luc2/indoor-positioning-cordova-plugin.git",
  "version": "0.0.1",
  "description": "Cordova plugin for indoor positioning",
  "keywords": [
    "cordova",
    "indoor",
    "positioning"
  ],
  "license": "MIT",
  "scripts": {
    "build": "NODE_ENV=${NODE_ENV:-production} rollup --config config/rollup.js",
    "test": "NODE_ENV=${NODE_ENV:-test} bin/ci",
    "test:unit": "NODE_ENV=${NODE_ENV:-test} karma start config/karma.js --single-run",
    "test:watch": "NODE_ENV=${NODE_ENV:-test} karma start config/karma.js --watch",
    "lint": "eslint src/ test/"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "eslint": "^4.17.0",
    "eslint-plugin-compat": "^2.2.0",
    "eslint-plugin-prettier": "^2.6.0",
    "karma": "^2.0.0",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.5",
    "karma-rollup-preprocessor": "^5.1.1",
    "karma-sinon": "^1.0.5",
    "mocha": "^5.0.0",
    "prettier": "^1.10.2",
    "rollup": "^0.55.3",
    "rollup-plugin-buble": "^0.18.0",
    "rollup-plugin-node-resolve": "^3.0.2",
    "sinon": "^4.2.2",
    "sinon-chai": "^2.14.0"
  },
  "dependencies": {}
}
